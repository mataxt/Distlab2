@model IEnumerable<Dist_Lab2.ViewModels.InboxViewModels>
@{
    ViewBag.Title = "Inbox Page";
}

<h2>@ViewBag.Title.</h2>
<h4>Check your inbox here</h4>

<hr />

<table class="table">
    <tr>
        <th>
            Html.DisplayNameFor(model => model.Senders)
        </th>
        <th></th>
    </tr>

foreach (var messages in ?) {
    <tr>
        <td>
            Html.DisplayFor(modelItem => messages.Senders)
        </td>
        <td>
            @Html.ActionLink("Details", "Details") |
            @Html.ActionLink("Delete", "Delete")
        </td>
    </tr>
}
</table>
